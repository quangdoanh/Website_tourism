header.header
  .inner-logo
    a(href=`/${adminPathName}/dashboard`)
      span DVTPD 
      span Admin
  .inner-wrap
    .inner-account
      .inner-avatar
        img(src=(account.avatar || ""), alt="")
      .inner-text
        .inner-name #{account.fullName}
        .inner-role #{account.roleName}
    button.inner-button-menu
      <i class="fa-solid fa-bars"></i>